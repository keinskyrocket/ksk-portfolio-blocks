{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/ksk-chart-bar",
	"version": "0.1.0",
	"title": "KSK Chart - Bar",
	"category": "design",
	"icon": "chart-bar",
	"description": "Chart - Bar block",
	"supports": {
		"html": true,
		"color": {},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"align": [ "wide", "full" ]
	},
	"textdomain": "ksk-chart-bar",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"chartValue": {
			"type": "string",
			"default": "20"
		}
	}
}
